{
  "name": "world-pharma-vn-backend",
  "version": "1.1.0",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "rootDir": "dist",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "_moduleAliases": {
    "@app": "./",
    "@utils": "./utils",
    "@modules": "./modules",
    "@core": "./core"
  },
  "scripts": {
    "copy-files": "cp ./src/core/casbin/model.conf ./dist/core/casbin;cp -r ./src/core/email/templates ./dist/core/email;",
    "build": "tsc; cp package.json dist; npm run copy-files",
    "start": "ts-node-dev --ignore-watch node_modules -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "createModule": "node src/commands/autoCreateFile.ts",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "eslint ./src --ext .js,.ts --fix",
    "semantic-release": "semantic-release",
    "migrate:create": "node src/commands/createFileMigrate.ts",
    "migrate:status": "node_modules/.bin/ts-node-dev node_modules/.bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:down": "node_modules/.bin/ts-node-dev node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts",
    "migrate": "node_modules/.bin/ts-node-dev node_modules/.bin/migrate-mongo up -f migrate-mongo-config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^8.3.6",
    "@commitlint/config-conventional": "^8.3.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^13.0.0",
    "@semantic-release/npm": "^11.0.2",
    "@types/body-parser": "1.19.5",
    "@types/cors": "2.8.17",
    "@types/cron": "2.4.0",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@types/lodash": "4.14.202",
    "@types/mongoose-aggregate-paginate-v2": "1.0.12",
    "@types/mongoose-sequence": "3.0.11",
    "@types/node": "20.10.5",
    "@types/xlsx-populate": "git+https://github.com/JanLoebel/types-xlsx-populate.git",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-sonarjs": "^0.23.0",
    "prettier": "3.1.1",
    "semantic-release": "22.0.12",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@aws-sdk/s3-request-presigner": "3.481.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "5.0.2",
    "@types/bluebird": "3.5.42",
    "@types/mongoose-paginate-v2": "^1.6.5",
    "@types/multer": "1.4.11",
    "@types/nodemailer": "6.4.14",
    "@types/uuid": "9.0.7",
    "aws-sdk": "2.1525.0",
    "axios": "^0.19.2",
    "bcrypt": "5.1.1",
    "bluebird": "3.7.2",
    "body-parser": "1.20.2",
    "casbin": "5.28.0",
    "casbin-mongoose-adapter": "5.3.1",
    "cors": "2.8.5",
    "cron": "3.1.6",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "handlebars": "^4.7.8",
    "hash.js": "1.1.7",
    "husky": "^4.3.8",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "migrate-mongo": "11.0.0",
    "module-alias": "2.2.3",
    "moment": "2.30.0",
    "moment-timezone": "0.5.43",
    "mongodb": "^6.4.0",
    "mongoose": "^6.12.2",
    "mongoose-aggregate-paginate-v2": "1.0.6",
    "mongoose-paginate-v2": "1.7.4",
    "mongoose-sequence": "6.0.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "6.9.8",
    "normalize-text": "2.4.1",
    "pnpm": "^8.13.1",
